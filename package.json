{
    "name": "drinks",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
        "dev:client": "vite",
        "dev:server": "tsx watch src/server/index.ts",
        "dev:full": "npm run build:client && npm run dev:server",
        "build": "npm run build:client && npm run build:server",
        "build:client": "tsc -b --noCheck && vite build",
        "build:server": "tsc src/server/index.ts src/lib/database.ts src/lib/types.ts --outDir dist-server --module ESNext --target ES2022 --moduleResolution node --allowSyntheticDefaultImports --esModuleInterop --declaration false",
        "start": "node dist-server/server/index.js",
        "start:prod": "npm run build && npm start",
        "kill": "fuser -k 5000/tcp",
        "optimize": "vite optimize",
        "preview": "vite preview",
        "test": "vitest run",
        "test:watch": "vitest"
    },
    "dependencies": {
        "@ai-sdk/openai": "^1.3.22",
        "@ai-sdk/react": "^1.2.12",
        "@azure/cosmos": "^4.4.1",
        "@octokit/rest": "^22.0.0",
        "@phosphor-icons/react": "^2.1.10",
        "@radix-ui/colors": "^3.0.0",
        "@radix-ui/react-alert-dialog": "^1.1.14",
        "@radix-ui/react-dialog": "^1.1.14",
        "@radix-ui/react-label": "^2.1.7",
        "@radix-ui/react-select": "^2.2.5",
        "@radix-ui/react-slider": "^1.3.5",
        "@radix-ui/react-slot": "^1.2.3",
        "@radix-ui/react-toggle": "^1.1.9",
        "@radix-ui/react-tooltip": "^1.2.7",
        "@tailwindcss/typography": "^0.5.16",
        "@tailwindcss/vite": "^4.1.10",
        "@types/sqlite3": "^5.1.0",
        "ai": "^4.3.16",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "express": "^5.1.0",
        "marked": "^15.0.12",
        "morgan": "^1.10.0",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-markdown": "^10.1.0",
        "react-router-dom": "^7.6.2",
        "sonner": "^2.0.5",
        "sqlite3": "^5.1.7",
        "tailwind-merge": "^3.3.1",
        "tw-animate-css": "^1.3.4",
        "vite-plugin-svgr": "^4.3.0",
        "zod": "^3.25.63"
    },
    "devDependencies": {
        "@tailwindcss/postcss": "^4.1.10",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.3.0",
        "@types/express": "^5.0.3",
        "@types/morgan": "^1.9.10",
        "@types/node": "^24.0.1",
        "@types/react": "^19.1.8",
        "@types/react-dom": "^19.1.6",
        "@types/supertest": "^6.0.3",
        "@vitejs/plugin-react": "^4.5.2",
        "@vitejs/plugin-react-swc": "^3.10.2",
        "concurrently": "^9.1.2",
        "dotenv": "^16.5.0",
        "globals": "^16.2.0",
        "jsdom": "^26.1.0",
        "supertest": "^7.1.1",
        "tailwindcss": "^4.1.10",
        "tsx": "^4.20.4",
        "typescript": "~5.8.3",
        "vite": "^6.3.5",
        "vitest": "^3.2.3"
    },
    "workspaces": {
        "packages": [
            "packages/*"
        ]
    }
}