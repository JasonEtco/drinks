{
    "name": "drinks",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
        "dev:client": "vite build --watch",
        "dev:server": "tsx watch src/server.ts",
        "build": "npm run build:client && npm run build:server",
        "build:client": "tsc -b --noCheck && vite build",
        "build:server": "tsc src/server.ts src/lib/database.ts src/lib/types.ts --outDir dist-server --module ESNext --target ES2022 --moduleResolution node --allowSyntheticDefaultImports --esModuleInterop --declaration false",
        "start": "node dist-server/server.js",
        "kill": "fuser -k 5000/tcp",
        "lint": "eslint .",
        "optimize": "vite optimize",
        "preview": "vite preview",
        "test": "vitest run",
        "test:watch": "vitest"
    },
    "dependencies": {
        "@octokit/rest": "^22.0.0",
        "@phosphor-icons/react": "^2.1.10",
        "@radix-ui/colors": "^3.0.0",
        "@radix-ui/react-alert-dialog": "^1.1.14",
        "@radix-ui/react-dialog": "^1.1.14",
        "@radix-ui/react-label": "^2.1.7",
        "@radix-ui/react-select": "^2.2.5",
        "@radix-ui/react-slider": "^1.3.5",
        "@radix-ui/react-slot": "^1.2.3",
        "@radix-ui/react-toggle": "^1.1.9",
        "@radix-ui/react-tooltip": "^1.2.7",
        "@tailwindcss/container-queries": "^0.1.1",
        "@tailwindcss/vite": "^4.1.8",
        "@types/sqlite3": "^5.1.0",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "express": "^5.1.0",
        "morgan": "^1.10.0",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-router-dom": "^7.6.1",
        "sonner": "^2.0.4",
        "sqlite3": "^5.1.7",
        "tailwind-merge": "^3.3.0",
        "tw-animate-css": "^1.3.2",
        "zod": "^3.25.42"
    },
    "devDependencies": {
        "@eslint/js": "^9.28.0",
        "@tailwindcss/postcss": "^4.1.8",
        "@types/express": "^5.0.2",
        "@types/morgan": "^1.9.9",
        "@types/node": "^22.15.29",
        "@types/react": "^19.1.6",
        "@types/react-dom": "^19.1.5",
        "@types/supertest": "^6.0.3",
        "@vitejs/plugin-react": "^4.5.0",
        "@vitejs/plugin-react-swc": "^3.10.0",
        "concurrently": "^9.1.2",
        "eslint": "^9.28.0",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.20",
        "globals": "^16.2.0",
        "supertest": "^7.1.1",
        "tailwindcss": "^4.1.8",
        "tsx": "^4.19.4",
        "typescript": "~5.8.3",
        "typescript-eslint": "^8.33.0",
        "vite": "^6.3.5",
        "vitest": "^3.1.4"
    },
    "workspaces": {
        "packages": [
            "packages/*"
        ]
    }
}
